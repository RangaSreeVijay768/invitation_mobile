{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2020",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "resolveJsonModule": true,
    "target": "es2020",
    "types": [
      "nodes"
    ],
    "paths": {
      "@core/*": [
        "src/app/core/*"
      ],
      "@basic/*": [
        "src/app/basic/*"
      ],
      "@images/*": [
        "src/app/images/*"
      ],
      "@events/*": [
        "src/app/events/*"
      ],
      "@signedin/*": [
        "src/app/signedin/*"
      ],
      "@user-profiles/*": [
        "src/app/user-profiles/*"
      ],
      "@users/*": [
        "src/app/users/*"
      ],
      "@node-modules/*": [
        "node_modules/*"
      ],
      "@environment/*": [
        "src/environments/*"
      ],
      "@angular/*": [
        "node_modules/@angular/*"
      ]
    },
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ]
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
